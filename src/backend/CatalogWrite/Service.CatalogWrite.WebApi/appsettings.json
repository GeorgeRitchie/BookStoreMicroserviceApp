{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"Service": {
		"CatalogWrite": {
			"BackgroundJobs": {
				"ProcessInboxMessages": {
					"IntervalInSeconds": 1,
					"RetryCount": 3,
					"BatchSize": 50
				},
				"ProcessOutboxMessages": {
					"IntervalInSeconds": 1,
					"RetryCount": 3,
					"BatchSize": 50
				}
			}
		}
	},
	"AllowedHosts": "*"
}
